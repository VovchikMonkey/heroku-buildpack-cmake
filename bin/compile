#!/bin/sh
# bin/compile BUILD_DIR CACHE_DIR ENV_DIR


cd /app
echo $(ls)

cd /app/docker
find . -type d
cd /app/bin
find . -type d

cd /app/tmp/cache/node/node_modules/opencv-build/opencv/
echo "cd opencv"
echo $(ls)
rm -rf opencv_contrib opencv
cd build/
rm *.txt *.cmake *.hpp *.tmp *.h
rm -rf CMakeFiles/ unix-install/ doc/ modules/ share/ downloads/ data/ 3rdparty/ opencv2/ lib64/ junk Makefile

#exit 1
